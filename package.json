{
	"name": "aws-geocode-mcp",
	"version": "0.0.3",
	"description": "Model Context Protocol server for geocoding services",
	"main": "dist/main.mjs",
	"type": "module",
	"bin": {
		"aws-geocode-mcp": "./dist/main.mjs"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsx src/main.ts --watch",
		"build": "esbuild src/main.ts --bundle --outfile=dist/main.mjs --platform=node --format=esm --target=node22",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"mcp",
		"geocoding",
		"amazon-location",
		"model-context-protocol"
	],
	"author": "kanahiro",
	"license": "MIT",
	"packageManager": "pnpm@10.5.2",
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.12.1",
		"zod": "^3.25.51"
	},
	"devDependencies": {
		"@types/node": "^24.0.0",
		"esbuild": "^0.25.5",
		"tsx": "^4.19.4"
	}
}
